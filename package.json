{
  "name": "pulsar-ai",
  "main": "./dist/main.js",
  "version": "0.0.1",
  "description": "Plugin AI pour Pulsar",
  "keywords": [],
  "repository": "https://github.com/mjerem34/pulsar-ai",
  "license": "MIT",
  "engines": {
    "pulsar": ">=1.0.0"
  },
  "scripts": {
    "build": "tsup lib/main.ts --format cjs --external pulsar",
    "dev": "tsup lib/main.ts --format cjs --external pulsar --watch"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.24",
    "@langchain/core": "^0.3.62",
    "@langchain/google-genai": "^0.0.11",
    "@langchain/openai": "^0.5.18",
    "atom": "^1.4.2",
    "highlight.js": "^11.9.0",
    "langchain": "^0.2.0",
    "marked": "^9.0.0"
  },
  "devDependencies": {
    "@types/atom": "^1.40.18",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "configSchema": {
    "panelVisible": {
      "type": "boolean",
      "default": false
    },
    "openaiApiKey": {
      "title": "OpenAI API Key",
      "description": "Your API Key for OpenAI Models",
      "type": "string",
      "default": "",
      "order": 1
    },
    "anthropicApiKey": {
      "title": "Anthropic API Key",
      "description": "Your API Key for Anthropic Models",
      "type": "string",
      "default": "",
      "order": 2
    },
    "mistralApiKey": {
      "title": "Mistral API Key",
      "description": "Your API Key for Mistral Models",
      "type": "string",
      "default": "",
      "order": 3
    },
    "defaultModel": {
      "title": "Default model",
      "description": "The default model",
      "type": "string",
      "default": "gpt-4o-mini",
      "enum": [
        "gpt-4o-mini",
        "gpt-4o",
        "claude-3-opus-latest",
        "claude-3-5-sonnet-latest",
        "claude-3-5-haiku-latest",
        "mistral-large",
        "mistral-medium",
        "mistral-small"
      ],
      "order": 4
    },
    "autocompleteEnabled": {
      "title": "Activer l'autocomplétion",
      "description": "Activer ou désactiver la fonctionnalité d'autocomplétion IA",
      "type": "boolean",
      "default": true,
      "order": 5
    },
    "autocompleteModel": {
      "title": "Modèle pour l'autocomplétion",
      "description": "Modèle d'IA à utiliser pour l'autocomplétion",
      "type": "string",
      "default": "gpt-4o-mini",
      "enum": [
        "gpt-4o-mini",
        "gpt-4o",
        "claude-3-5-haiku-latest",
        "mistral-small"
      ],
      "order": 6
    },
    "autocompleteDelay": {
      "title": "Délai d'autocomplétion (ms)",
      "description": "Délai en millisecondes avant de déclencher l'autocomplétion",
      "type": "integer",
      "default": 500,
      "minimum": 100,
      "maximum": 2000,
      "order": 7
    }
  },
  "deserializers": {
    "pulsar-ai/PulsarAIView": "deserializePulsarAIView"
  }
}
