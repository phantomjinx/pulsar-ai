{
  "name": "pulsar-ai",
  "main": "./lib/main",
  "version": "0.0.1",
  "description": "Plugin AI pour Pulsar",
  "keywords": [],
  "repository": "https://github.com/mjerem34/pulsar-ai",
  "license": "MIT",
  "engines": {
    "pulsar": ">=1.0.0"
  },
  "dependencies": {
    "marked": "^9.0.0",
    "highlight.js": "^11.9.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "configSchema": {
    "panelVisible": {
      "type": "boolean",
      "default": false
    },
    "openaiApiKey": {
      "title": "OpenAI API Key",
      "description": "Votre clé API pour OpenAI (requis pour les modèles GPT)",
      "type": "string",
      "default": "",
      "order": 1
    },
    "anthropicApiKey": {
      "title": "Anthropic API Key",
      "description": "Votre clé API pour Anthropic (requis pour les modèles Claude)",
      "type": "string",
      "default": "",
      "order": 2
    },
    "mistralApiKey": {
      "title": "Mistral API Key",
      "description": "Votre clé API pour Mistral (requis pour les modèles Mistral)",
      "type": "string",
      "default": "",
      "order": 3
    },
    "defaultModel": {
      "title": "Modèle par défaut",
      "description": "Le modèle à utiliser par défaut",
      "type": "string",
      "default": "gpt-4o-mini",
      "enum": [
        "gpt-4o-mini",
        "gpt-4o",
        "claude-3-opus-latest",
        "claude-3-5-sonnet-latest",
        "claude-3-5-haiku-latest",
        "mistral-large",
        "mistral-medium",
        "mistral-small"
      ],
      "order": 4
    }
  },
  "deserializers": {
    "pulsar-ai/PulsarAIView": "deserializePulsarAIView"
  }
}
